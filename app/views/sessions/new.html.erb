<h1>Sign In</h1>
<%= form_for :user do |f| %>
  <%= f.label :email %>
  <%= f.text_field :email, placeholder: 'jane@fake.com' %><br>
  <%= f.label :password %>
  <%= f.password_field :password, placeholder: 'password' %><br><br>

  <%= f.submit 'Sign In' %>
<% end %>

<br><%= link_to 'Log in with Facebook!', '/auth/facebook' %>
